# API Keys Configuration
# Copy this file to .env and fill in your actual keys
# NEVER commit .env to git!

# Deepgram API Key (Speech-to-Text)
# Get your key from: https://console.deepgram.com/
DEEPGRAM_API_KEY=your_deepgram_key_here

# ElevenLabs API Key (Text-to-Speech)
# Get your key from: https://elevenlabs.io/app/settings/api-keys
ELEVENLABS_API_KEY=your_elevenlabs_key_here

# Voice Configuration (optional)
ELEVENLABS_VOICE_ID=21m00Tcm4TlvDq8ikWAM  # Rachel voice
ELEVENLABS_MODEL=eleven_flash_v2_5

# LiveKit Configuration (optional, for conversational mode)
LIVEKIT_URL=ws://localhost:7880
LIVEKIT_API_KEY=devkey
LIVEKIT_API_SECRET=secret
